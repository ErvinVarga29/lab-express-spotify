<div class="tracksHeaders">

    <h2>Track Good Music</h2>

</div>

<div class="tracksContainer">

    {{#each tracks}}

<div class="indTrack" >

    <h5 class="trackName">{{name}}</h5>

</div>

{{#if preview_url}}

<audio controls>

    <source src="{{preview_url}}">

</audio>

{{else}}

<h5 class="noPrev">No music for you</h5>

{{/if}}

</div>

{{/each}}
</div>